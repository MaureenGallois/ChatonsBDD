@model Dictionary<string, List<string>>

<h3>Bonjour</h3>

<ul>

    @foreach (var item in Model)
    {
        <li>
            @item.Key:
            <ul>
                @foreach (var value in item.Value)
                {
                    <li>@value</li>
                }
            </ul>
        </li>
    }
</ul>
